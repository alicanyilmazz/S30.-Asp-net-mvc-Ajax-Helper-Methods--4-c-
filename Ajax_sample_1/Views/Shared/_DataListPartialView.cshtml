@model List<string>


@foreach (string item in Model)
{
    <li id="id_@Model.IndexOf(item)" class="list-group-item">

        @Ajax.ActionLink("Delete",
       "RemoveData",
       new { id=Model.IndexOf(item)},
       new AjaxOptions() {LoadingElementId= "loading_bar",LoadingElementDuration=300,HttpMethod="GET",InsertionMode=InsertionMode.ReplaceWith,UpdateTargetId="id_"+ @Model.IndexOf(item) },
       new {@class= "btn btn-danger btn-xs" })
        &nbsp;@item

    </li>
}
